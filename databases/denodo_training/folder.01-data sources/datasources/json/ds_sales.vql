# REQUIRES-PROPERTIES-FILE - # Do not remove this comment!
CREATE OR REPLACE DATASOURCE JSON ds_sales
    FOLDER = '/01-data sources'
    ROUTE HTTP 'http.CommonsHttpClientConnection,120000' GET '${databases.denodo_training.folder.01-data sources.datasources.json.ds_sales.ROUTE.HTTP.URL}'
    HEADERS (
        'Accept'='application/json', 
        'Content-type'='application/json'
    )
    AUTHENTICATION BASIC ( 
      USER '${databases.denodo_training.folder.01-data sources.datasources.json.ds_sales.ROUTE.HTTP.USER}'
      PASSWORD '${databases.denodo_training.folder.01-data sources.datasources.json.ds_sales.ROUTE.HTTP.PASSWORD}' ${databases.denodo_training.folder.01-data sources.datasources.json.ds_sales.ROUTE.HTTP.PASSWORD.ENCRYPTED} )
    PROXY OFF;

